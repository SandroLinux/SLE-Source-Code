{
 "name": "Sandro Linux Extension",
 "description": "An extension which lets you access my content",
 "version": "1.1",
 "manifest_version": 2,
 "icons": {"48": "sandrolinuxicon48.png"},
 "content_scripts": [
 {
    "matches": ["<all_urls>"],
    "js": ["extension.js"]
 }
 ],
 "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
 }
}
